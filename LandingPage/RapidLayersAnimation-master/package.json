{
  "name": "RapidLayersAnimation",
  "version": "1.0.0",
  "description": "A rapid layers animation of images as a splash intro or page transition on a website.",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.html --open",
    "clean": "rm -rf dist/*",
    "build:parcel": "parcel build src/index.html --no-content-hash --no-minify --no-source-maps --public-url ./",
    "build": "npm run clean && npm run build:parcel"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/codrops/RapidLayersAnimation.git"
  },
  "keywords": [],
  "author": "Codrops",
  "license": "MIT",
  "homepage": "[HOMEPAGE]",
  "bugs": {
    "url": "https://github.com/codrops/RapidLayersAnimation/issues"
  },
  "dependencies": {
    "gsap": "^3.2.6",
    "imagesloaded": "^4.1.4",
    "parcel": "^1.12.4"
  }
}
